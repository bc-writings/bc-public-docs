\usepackage{tikz}
\usetikzlibrary{decorations.pathmorphing}

\newcommand{\mytikzrel}{
  \mathrel{%
    \raisebox{.3ex}{%
      \begin{tikzpicture}
        \draw[
          <->,
          line cap = round,
          decorate,
          decoration = {
            zigzag,
            segment length = 1.7,
            amplitude      = .8,
            pre length     = 2pt,
            post length    = 2pt
          }
        ] (0,0) -- (.3,0);
      \end{tikzpicture}%
    }%
  }
}
